{
  "name": "monkeytips/monkeytips-walletd-rpc-php",
  "description": "A PHP wrapper for the monkeytips walletd daemon RPC interface",
  "keywords": [
    "monkeytips",
    "PHP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "crappyrules",
      "email": "crappyrules@protonmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "monkeytips\\": "src/monkeytips"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
